TARGET = infinit limit celsius2fahr fahr2celsius charNumber triangle arithmetics increment literal

all : ${TARGET}

infinit : infinit.c
	cc -o infinit infinit.c

limit : $@
celsius2fahr : $@
fahr2celsius : $@
charNumber : $@
triangle : $@
arithmetics : $@
increment : $@
literal : $@

clean :
	rm ${TARGET}
